<!DOCTYPE html>
<html>
<body>
<script>
    var count = window.prompt('Please enter amount of natural numbers.', 0);
    var arr = [];
    var currentNumber = 2;

    while (count !== 0) {
        if (isNatural(currentNumber)) {
            arr.push(currentNumber);
            count--;
        }
        currentNumber++;
    }

    function isNatural(number) {
        for (var i = 2; i <= number / 2; i++) {
            if (number % i === 0) {
                return false;
            }
        }
        return true;
    }
    alert(arr);
</script>
</body>
</html>
